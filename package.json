{
  "name": "github",
  "description": "organization level configuration repository",
  "license": "MIT",
  "private": true,
  "repository": "travi-test/.github",
  "bugs": "https://github.com/travi-test/.github/issues",
  "homepage": "https://github.com/travi-test/.github#readme",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "scripts": {
    "test": "npm-run-all --print-label --parallel lint:*",
    "lint:lockfile": "lockfile-lint --path package-lock.json --type npm --validate-https --allowed-hosts npm",
    "lint:md": "remark . --frail",
    "generate:md": "remark . --output",
    "lint:sensitive": "ban",
    "lint:peer": "npm ls >/dev/null",
    "prepare": "husky install"
  },
  "devDependencies": {
    "ban-sensitive-files": "1.10.1",
    "commitlint-config-travi": "1.4.44",
    "cz-conventional-changelog": "3.3.0",
    "husky": "8.0.3",
    "lockfile-lint": "4.12.1",
    "npm-run-all2": "6.1.1",
    "remark-cli": "12.0.0",
    "remark-preset-lint-travi": "6.0.3",
    "remark-toc": "9.0.0"
  }
}
